<settings>
  <margins>0</margins>
  <max_series>1400</max_series>
  <text_size>9</text_size>
  <font>arial</font>
  <add_time_stamp>true</add_time_stamp>
  <number_format>
		<decimal_separator>.</decimal_separator>
		<thousand_separator>,</thousand_separator>
		<digits_after_decimal>
			<data>2</data>
		</digits_after_decimal>
		<letters>	
			<letter number="1000000"> M</letter>
			<letter number="1000000000">B</letter>
		</letters>
	</number_format>
  <date_formats>        
    <x_axis>
			<minutes>&amp;nbsp;</minutes>
	        <hours>hh:00</hours>
			<days>DD month</days>
			<months>month</months>
			<years>YYYY</years>
		</x_axis>
		<legend>
			<minutes>hh:mm</minutes>
	        <hours>DD month</hours>
			<weeks>month DD - YYYY</weeks>
			<days>DD month</days>
			<months>month</months>
			<years>YYYY</years>
		</legend>
  </date_formats>
  <data_sets>     
    <data_set>
      <title>Dolar</title>
      <short>Dolar</short>
      <color>1584d5</color>
      <file_name>/mercados/GraficosServlet?tipo=DIVISAS</file_name>
      <csv>
        <reverse>true</reverse>
        <separator>,</separator>
        <date_format>YYYY-MM-DD hh:mm</date_format>
        <decimal_separator>.</decimal_separator>
        <columns>
          <column>date</column>
          <column>close</column>
          <column>volumen</column>
          <column>ant</column>
          </columns>
      </csv>
    </data_set>
  </data_sets>
  <charts>
    <chart>
      <height>100</height>
      <title></title>
      <border_color>#CCCCCC</border_color>
      <border_alpha>0</border_alpha>
      <grid>
        <x>
          <alpha>10</alpha>
          <dashed>false</dashed>
		  <color>dae1e7</color>
        </x>
        <y_left>
          <enabled>true</enabled>
          <alpha>30</alpha>
          <dashed>false</dashed>
		  <color>dae1e7</color>
        </y_left>
		 <y_right>
          <enabled>true</enabled>
          <alpha>30</alpha>
          <dashed>false</dashed>
		  <color>dae1e7</color>
        </y_right>
      </grid>
      <values>
        <x>
          <enabled>true</enabled>
		  <bg_color>eaeaea</bg_color>
		  <text_color>333333</text_color>
		  <text_size>8</text_size>
        </x>
		<y_left>
          <enabled>true</enabled>
		  <bg_color>eaeaea</bg_color>
		  <text_color>000000</text_color>
		  <text_size>9</text_size>
        </y_left>
		<y_right>
          <enabled>true</enabled>
		  <bg_color>eaeaea</bg_color>
		  <text_color>777777</text_color>
		  <text_size>9</text_size>
        </y_right>
      </values>
      <legend>
        <enabled>true</enabled>
        <positive_color>7f8da9</positive_color>
        <negative_color>db4c3c</negative_color>
        <show_date>true</show_date>
		<key_size>6.7</key_size>
      </legend>
      <column_width>50</column_width>
      <graphs>
        <graph>
		  <axis>left</axis>
          <type>line</type>
          <data_sources>
            <close>close</close>
          </data_sources>
          <period_value/>
          <color>1584d5</color>
          <compare_source>close</compare_source>
          <cursor_color>1584d5</cursor_color>
          <fill_alpha>0</fill_alpha>
          <legend>
            <date key="true" title="true">
            	<![CDATA[{close}]]>
            </date>
          </legend>
        </graph>
		<graph>
			<axis>right</axis>
			<type>column</type>
			<data_sources>
				<close>volumen</close>
			</data_sources>
			<period_value>average</period_value>
			<color>cccccc</color>
			<cursor_color>aaaaaa</cursor_color>
			<fill_alpha>15</fill_alpha>
			<legend>
				<date key="true" title="false">
					<![CDATA[Cantidad: {average}]]>
				</date>  
			</legend>
		</graph>
      </graphs>
      <comparing>
        <recalculate>false</recalculate>    
      </comparing>
    </chart>	
  </charts>
  <data_set_selector>
    <enabled>false</enabled>
  </data_set_selector>
  <period_selector>
    <button>
      <bg_color_hover>FFFFFF</bg_color_hover>
      <bg_color_selected>E4F7FD</bg_color_selected>
      <text_color_selected>000000</text_color_selected>
    </button>
    <periods>
      <period type="hh" count="7" >D&#237;a</period>
    </periods>
    <custom_period_enabled>false</custom_period_enabled>
    <enabled>false</enabled>
    <!--periods_title>Zoom:</periods_title-->
    <!--custom_period_title>Custom period:</custom_period_title-->
  </period_selector>
  <header>
    <enabled>false</enabled>
  </header>
  <plot_area>
    <border_color>b6bece</border_color>
  </plot_area>
  <scroller>
    <graph_data_source>close</graph_data_source>
    <resize_button_style>dragger</resize_button_style>
    <enabled>false</enabled>
    <playback>
      <enabled>false</enabled>
      <color>002b6d</color>
      <color_hover>db4c3c</color_hover>
      <speed>3</speed>
      <max_speed>10</max_speed>
      <loop/>
      <speed_indicator>
        <color>002b6d</color>
      </speed_indicator>
    </playback>
  </scroller>
</settings>